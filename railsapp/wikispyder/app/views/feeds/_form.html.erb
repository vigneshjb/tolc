  </div>
  <div class="field">
    <%= f.label :down_vote_count %><br>
    <%= f.number_field :down_vote_count %>
  </div>
  <div class="field">
    <%= f.label :feed_type %><br>
    <%= f.text_field :feed_type %>
  </div>
  <div class="field">
    <%= f.label :feed_content %><br>
    <%= f.text_field :feed_content %>
  </div>
  <div class="field">
    <%= f.label :interest %><br>
    <%= f.number_field :interest %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
